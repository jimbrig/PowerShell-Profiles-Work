          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false" position="named" aliases="">
          <maml:name>UserName</maml:name>
          <maml:description>
            <maml:para>Specifies the local user account name to use for assigned access. This cannot be a domain account or an administrator account. </maml:para>
