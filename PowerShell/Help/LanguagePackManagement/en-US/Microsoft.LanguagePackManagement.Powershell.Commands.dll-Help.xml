  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10" xmlns:MSHelp="http://msdn.microsoft.com/mshelp">
    <command:details>
      <command:name>Install-Language</command:name>
      <command:verb>Install</command:verb>
      <command:noun>Language</command:noun>
      <maml:description>
        <maml:para>Installs a language onto a device.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Downloads and installs the available language components for the specified language onto a device. </maml:para>
      <maml:para>You can also change your Display Language by using the International module commands (`Set-WinUILanguageOverride &lt;lnstalled Language&gt;`)
</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Install-Language</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByValue)" position="0" aliases="LanguageId, LanguageTag">
          <maml:name>Language</maml:name>
          <maml:description>
            <maml:para>The bcp47 tag of the language that you're installing.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="none">
          <maml:name>AsJob</maml:name>
          <maml:description>
            <maml:para>If specified, this parameter will execute the language installation as an async PowerShell job (/powershell/module/microsoft.powershell.core/about/about_jobs).</maml:para>
          </maml:description>
          <dev:type>
            <maml:name>SwitchParameter</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>False</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="ApplyToSettings, ApplyToInternationalSettings, CopyToInternationalSettings">
          <maml:name>CopyToSettings</maml:name>
          <maml:description>
            <maml:para>If specified, this parameter sets the System and Default Device Settings (Windows Display Language, regional and locale formats) to the installed language following the installation. </maml:para>
            <maml:para>After a language is added with this command, you have to restart the device or login again for changes to take effect. Once you log back in, you can change your Windows Display Language in  Settings App to change the device UI language.</maml:para>
          </maml:description>
          <dev:type>
            <maml:name>SwitchParameter</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>False</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="none">
          <maml:name>ExcludeFeatures</maml:name>
          <maml:description>
            <maml:para>If specified, the associated language Features on Demand won't be installed.</maml:para>
          </maml:description>
          <dev:type>
            <maml:name>SwitchParameter</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>False</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters>
      <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="none">
        <maml:name>AsJob</maml:name>
        <maml:description>
          <maml:para>If specified, this parameter will execute the language installation as an async PowerShell job (/powershell/module/microsoft.powershell.core/about/about_jobs).</maml:para>
        </maml:description>
        <command:parameterValue required="false" variableLength="false">SwitchParameter</command:parameterValue>
        <dev:type>
          <maml:name>SwitchParameter</maml:name>
          <maml:uri />
        </dev:type>
        <dev:defaultValue>False</dev:defaultValue>
      </command:parameter>
      <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="ApplyToSettings, ApplyToInternationalSettings, CopyToInternationalSettings">
        <maml:name>CopyToSettings</maml:name>
        <maml:description>
          <maml:para>If specified, this parameter sets the System and Default Device Settings (Windows Display Language, regional and locale formats) to the installed language following the installation. </maml:para>
          <maml:para>After a language is added with this command, you have to restart the device or login again for changes to take effect. Once you log back in, you can change your Windows Display Language in  Settings App to change the device UI language.</maml:para>
        </maml:description>
        <command:parameterValue required="false" variableLength="false">SwitchParameter</command:parameterValue>
        <dev:type>
          <maml:name>SwitchParameter</maml:name>
          <maml:uri />
        </dev:type>
        <dev:defaultValue>False</dev:defaultValue>
      </command:parameter>
      <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="named" aliases="none">
        <maml:name>ExcludeFeatures</maml:name>
        <maml:description>
          <maml:para>If specified, the associated language Features on Demand won't be installed.</maml:para>
        </maml:description>
        <command:parameterValue required="false" variableLength="false">SwitchParameter</command:parameterValue>
        <dev:type>
          <maml:name>SwitchParameter</maml:name>
          <maml:uri />
        </dev:type>
        <dev:defaultValue>False</dev:defaultValue>
      </command:parameter>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByValue)" position="0" aliases="LanguageId, LanguageTag">
        <maml:name>Language</maml:name>
        <maml:description>
          <maml:para>The bcp47 tag of the language that you're installing.</maml:para>
        </maml:description>
        <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
        <dev:type>
          <maml:name>String</maml:name>
          <maml:uri />
        </dev:type>
        <dev:defaultValue>None</dev:defaultValue>
      </command:parameter>
    </command:parameters>
    <command:inputTypes>
      <command:inputType>
        <dev:type>
          <maml:name>System.String</maml:name>
        </dev:type>
        <maml:description>
          <maml:para></maml:para>
        </maml:description>
      </command:inputType>
    </command:inputTypes>
    <command:returnValues>
      <command:returnValue>
        <dev:type>
          <maml:name>Microsoft.LanguagePackManagement.Powershell.Commands.InstallLanguage+InstallLanguageJob</maml:name>
        </dev:type>
        <maml:description>
          <maml:para></maml:para>
        </maml:description>
      </command:returnValue>
      <command:returnValue>
        <dev:type>
          <maml:name>System.Collections.Generic.List&lt;Microsoft.LanguagePackManagement.Powershell.Commands.InstalledLanguage&gt;</maml:name>
        </dev:type>
        <maml:description>
          <maml:para></maml:para>
        </maml:description>
      </command:returnValue>
    </command:returnValues>
    <maml:alertSet>
      <maml:alert />
    </maml:alertSet>
    <command:examples>
      <command:example>
        <maml:title>------------ Example 1: Add a language to a device ------------</maml:title>
        <dev:code>Install-Language ja-JP</dev:code>
        <dev:remarks>
          <maml:para>This command adds the Japanese language to a device.</maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>
    <command:relatedLinks>
      <maml:navigationLink>
        <maml:linkText>Online Version:</maml:linkText>
        <maml:uri>https://learn.microsoft.com/powershell/module/languagepackmanagement/install-language?view=windowsserver2022-ps</maml:uri>
      </maml:navigationLink>
      <maml:navigationLink>
